{
    "themeName": "Old Micasend theme",
    "author": "MagicTINTIN",
    "themeVersion": 2,
    "compatibilityVersion": 2,

    "settings": {},
    "maxMessages":20,
    "formatText":false,

    "Advanced settings": "Do not modify lines below unlesss you know what you are doing !",
    "prompt": [],
    "message": [
        {
            "type":"if",
            "condition":"?isDeleted",
            "true": [
                {
                    "type":"BREAKDISPLAY"
                }
            ],
            "false":[]
        },
        {
            "type":"if",
            "condition":"?isOffline",
            "true": [
                {
                    "type":"BREAKDISPLAY"
                }
            ],
            "false":[]
        },
        {
            "type":"NEWLINE"
        },
        {
            "type":"print",
            "style":[],
            "print":" "
        },
        {
            "type":"if",
            "condition":"?botRank",
            "true": [
                {
                    "type":"print",
                    "style":[],
                    "print":"["
                },
                {
                    "type":"print",
                    "style":[ "RED_NORMAL_COLOR" ],
                    "print":"Bot"
                },
                {
                    "type":"print",
                    "style":[],
                    "print":"] "
                }
            ],
            "false":[]
        },
        {
            "type":"if",
            "condition":"?moderatorRank",
            "true": [
                {
                    "type":"print",
                    "style":[],
                    "print":"["
                },
                {
                    "type":"print",
                    "style":[ "RED_NORMAL_COLOR" ],
                    "print":"Mod"
                },
                {
                    "type":"print",
                    "style":[],
                    "print":"] "
                }
            ],
            "false":[]
        },
        {
            "type":"if",
            "condition":"?adminRank",
            "true": [
                {
                    "type":"print",
                    "style":[],
                    "print":"["
                },
                {
                    "type":"print",
                    "style":[ "RED_NORMAL_COLOR" ],
                    "print":"Admin"
                },
                {
                    "type":"print",
                    "style":[],
                    "print":"] "
                }
            ],
            "false":[]
        },
        {
            "type":"if",
            "condition":"?isVerified",
            "true": [
                {
                    "type":"if",
                    "condition":"?isVerified",
                    "true": [
                        {
                            "type":"print",
                            "style":[ "GREEN_NORMAL_COLOR", "BOLD" ],
                            "print":"$MAUTHOR"
                        },
                        {
                            "type":"if",
                            "condition":"?isModeratorMode",
                            "true": [
                                {
                                    "type":"print",
                                    "style":[ "YELLOW_NORMAL_COLOR", "BOLD" ],
                                    "print":"($IDMAUTHOR)"
                                }
                            ],
                            "false":[]
                        }
                    ],
                    "false":[]
                }
            ],
            "false":[
                {
                    "type":"print",
                    "style":[ "RED_NORMAL_COLOR" ],
                    "print":"$MAUTHOR"
                }
            ]
        },
        {
            "type":"print",
            "style":[],
            "print":" $DATETIME"
        },
        {
            "type":"if",
            "condition":"?isModeratorMode",
            "true": [
                {
                    "type":"print",
                    "style":[ "YELLOW_NORMAL_COLOR" ],
                    "print":" ($MSGID)"
                }
            ],
            "false":[]
        },
        {
            "type":"NEWLINE"
        },
        {
            "type":"print",
            "style":[ "BLUE_NORMAL_COLOR" ],
            "print":" $MESSAGECONTENT"
        },
        {
            "type":"NEWLINE"
        }
    ],
    "messageInput": {
        "style":[]
    },
    "groupInput": {
        "style":[]
    },
    "replyInput": {
        "style":[]
    },
    "mention": {
        "style":[]
    }
}