{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CMake Debug MicaClient",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/debug/MicaClient",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "/usr/bin/gdb"
    },
